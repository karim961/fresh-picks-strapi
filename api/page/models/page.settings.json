{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "name": "Page",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "Sections": {
      "type": "dynamiczone",
      "components": [
        "section.carousel",
        "section.repeatable-hero-banner",
        "section.hero-banner",
        "section.lets-talk",
        "component.paragraph-block",
        "section.image-block"
      ]
    },
    "link": {
      "type": "string"
    },
    "background": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false
    }
  }
}
